  </sql>
  <sql id="Base_Column_List" >
    MENU_ID, MENU_CODE, MENU_NAME, MENU_DESC, MENU_URL, MENU_TYPE, MENU_DIC_CODE, MENU_PARENT_CODE, 
    MENU_SORT, MENU_IS_DYNAMIC_FORM, MENU_FORM_ID, MENU_SEQ, OPERATOR_NAME, OPERATOR_TIME, 
