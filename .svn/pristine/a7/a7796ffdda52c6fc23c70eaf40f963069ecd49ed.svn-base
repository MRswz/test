function initSelectForShenPi(e){
	if(e=='通过'){
		//approvalopinion
		var columns = [];
		for(var i=0;i<1;i++){
			var column = {};
			column.formColumnId = '';//字段ID
			column.formColumnShowType = 'true';//是否空值
