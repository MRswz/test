			checkvalue2 = checkCommon($(this));
			if(vtype.indexOf("maxLength")>-1 || vtype.indexOf("minLength")>-1){
				checkvalue3 = checkLength($(this));
			}
		}
		
		if(checkvalue1==true&&checkvalue2==true&&checkvalue3==true){
			writeCheckErrorMessage($(this),"");
		}
	});  
