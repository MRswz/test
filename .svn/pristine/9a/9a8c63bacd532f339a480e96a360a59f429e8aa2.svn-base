<div class="modal" ng-controller="KisBpmTimeoutPopupCtrl" data-onShow="showValue">
    <div class="modal-dialog" style="width: 400px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-click="close()">&times;</button>
                <!-- <label translate>PROPERTY.ASSIGNMENT.TITLE</label> -->
                <label translate>timeout type</label>
            </div>
            <div class="modal-body" style="background: #efefef;">
			  	<label>
				  <input type="radio" name="timeout" value="none" ng-model="timeout.id" />不处理
				</label><br/>
				<label>
				  <input type="radio" name="timeout" value="skip" ng-model="timeout.id" />跳过
				</label><br/>
				<label>
				  <input type="radio" name="timeout" value="manual_assign" ng-model="timeout.id" />手动指定处理人
				</label>
            </div>
            <div class="modal-footer">
                <button ng-click="close()" class="btn btn-primary" translate>ACTION.CANCEL</button>
                <button ng-click="save()" class="btn btn-primary" translate>ACTION.SAVE</button>
            </div>
        </div>
    </div>
</div>