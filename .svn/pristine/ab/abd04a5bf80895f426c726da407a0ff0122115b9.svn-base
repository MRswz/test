package com.myehr.pojo.sysParam;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpSession;

public class SysRequestParam {
	HttpServletRequest request;
	HttpSession session ;
	String isView;//request.getIsView();
	String contextPath;//request.getContextPath()
	String _workflowBusPrimaryKeyParamName;//request.getAttribute("_workflowBusPrimaryKeyParamName");
	String isInit;//request.getParameter("isInit");
	String path;//request.getRealPath("/jsp/formbuild/")
	public HttpSession getSession() {
		return session;
	}
	public void setSession(HttpSession session) {
		this.session = session;
	}
	public String getIsView() {
		return isView;
	}
	public void setIsView(String isView) {
		this.isView = isView;
	}
	public String getContextPath() {
		return contextPath;
	}
	public void setContextPath(String contextPath) {
		this.contextPath = contextPath;
	}
	public HttpServletRequest getRequest() {
		return request;
	}
	public void setRequest(HttpServletRequest request) {
		this.request = request;
	}
	public String get_workflowBusPrimaryKeyParamName() {
		return _workflowBusPrimaryKeyParamName;
	}
	public void set_workflowBusPrimaryKeyParamName(
			String _workflowBusPrimaryKeyParamName) {
		this._workflowBusPrimaryKeyParamName = _workflowBusPrimaryKeyParamName;
	}
	public String getIsInit() {
		return isInit;
	}
	public void setIsInit(String isInit) {
		this.isInit = isInit;
	}
	public String getPath() {
		return path;
	}
	public void setPath(String path) {
		this.path = path;
	}
	
	
}
